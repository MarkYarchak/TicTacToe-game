.board-cell {
  display: flex;
  justify-content: center;
  align-items: center;
  background-color: white;
  width: 100%;
  height: 100%;
  img {
    width: 50%;
    height: auto;
  }
}

.cell-empty-circle {
  width: 50%;
  height: 50%;
  border-radius: 50%;
  background-color: #fff;
  border: 5px solid black;
}
